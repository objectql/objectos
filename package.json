  "license": "AGPL-3.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "better-sqlite3",
      "esbuild",
      "sqlite3",
      "unrs-resolver"
    ],
    "patchedDependencies": {
      "@objectstack/cli@0.8.0": "patches/@objectstack__cli@0.8.0.patch"
    },
    "ignoredBuiltDependencies": [
      "sharp"
    ]
  },
  "scripts": {
    "dev": "pnpm --filter @objectos/plugin-server dev",
    "server": "pnpm --filter @objectos/plugin-server dev",
    "build": "tsc -b && pnpm -r build",
    "start": "pnpm --filter @objectos/plugin-server start:prod",
    "test": "pnpm -r test",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm run build && changeset publish",
    "site:dev": "pnpm --filter @objectos/site dev",
    "site:build": "pnpm --filter @objectos/site build",
    "site:preview": "pnpm --filter @objectos/site start",
    "web:dev": "pnpm --filter @objectos/web dev",
    "web:build": "pnpm --filter @objectos/web build",
    "web:start": "pnpm --filter @objectos/web start",
    "objectstack:serve": "objectstack serve"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@objectstack/cli": "^0.8.0",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.2.0",
    "@types/supertest": "^6.0.3",
    "concurrently": "^9.2.1",
    "jest": "^30.2.0",
    "js-yaml": "^4.1.1",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "vitepress": "^1.6.4"
  },
  "version": "0.1.0",
  "dependencies": {
    "@objectql/core": "^4.0.3",
    "@objectql/driver-mongo": "^4.0.3",
    "@objectql/driver-sql": "^4.0.3",
    "@objectql/platform-node": "^4.0.3",
    "@objectql/starter-basic": "^1.8.4",
    "@objectql/starter-enterprise": "^1.8.4",
    "@objectstack/spec": "0.8.2",
    "build": "^0.1.4"
  }
