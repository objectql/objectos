{
  "name": "@objectos/jobs",
  "version": "0.1.0",
  "type": "module",
  "license": "AGPL-3.0",
  "description": "Job queue and scheduling plugin for ObjectOS - background processing, cron jobs, and job monitoring",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean && tsc --emitDeclarationOnly --declaration",
