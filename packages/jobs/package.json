    "lint": "eslint src/",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@objectstack/runtime": "^3.0.1",
    "@objectstack/spec": "3.0.1",
    "cron-parser": "^4.9.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@objectql/core": "^4.2.0"
  }
}
